    "mini-css-extract-plugin": "2.6.1",
    "postcss": "8.2.4",
    "postcss-loader": "7.0.0",
    "stats-webpack-plugin": "0.7.0",
    "stylelint": "14.9.1",
    "stylelint-config-css-modules": "4.1.0",
    "stylelint-config-standard": "26.0.0",
    "terser-webpack-plugin": "2.3.8",
    "webpack": "4.46.0",
